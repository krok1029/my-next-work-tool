{
  "name": "my-next-work-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bunx prisma generate --schema=prisma/dev/schema.prisma && bun --bun run next dev",
    "build": "bunx prisma generate --schema=prisma/production/schema.prisma && bun run next build",
    "start": "dotenv -e .env.production bun run next start",
    "lint": "bun --bun run next lint",
    "migrate:dev": "bun x prisma migrate dev --schema=prisma/dev/schema.prisma --name init",
    "migrate:prod": "dotenv -e .env.production -- bunx prisma migrate deploy --schema=prisma/production/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.435.0",
    "next": "14.2.5",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "prisma": "^5.18.0",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
